  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "test:spec": "mocha testSetup.js \"tests/**/*.test.js\" || echo Tests failed",
    "lint": "eslint ./**/*.js || echo ESLint found some errors.",
    "eslint-find-option-rules": "eslint-find-rules -u .eslintrc"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config build/webpack.config.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
